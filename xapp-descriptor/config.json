{
        "name": "id-d",
        "version": "1.0.2",
        "APP_NAMESPACE": "djamel",
        "containers": [
            {
                "name": "idinsert",
                "image": {
                    "registry": "registry.hub.docker.com",
                    "name": "djamel1997/id",
                    "tag": "1.0"
                },
                "command": ["sh", "-c", "python insert.py"]
            },
            {
                "name": "id",
                "image": {
                    "registry": "registry.hub.docker.com",
                    "name": "djamel1997/id",
                    "tag": "1.0"
                }
            }
        ],
        "appenv": {
            "INFLUXDB_PASSWORD": "CemexAw3adl4jv0EKEiYO26Smjx8TmOOQEpxQd5Wcy51EX7Q1O8qrcic1JGCL8mEG2jS0-xExW4T-6TtkYldCw=="
        },
        "messaging": {
            "ports": [
                {
                    "name": "http",
                    "container": "id",
                    "port": 8080,
                    "description": "http service"
                },
                {
                    "name": "rmr-data",
                    "container": "id",
                    "port": 4560,
                    "txMessages": ["FBS_ANOMALY_UPDATE"],
                    "rxMessages": ["FBS_ANOMALY_ACK"],
                    "policies": [],
                    "description": "rmr receive data port for id"
                },
                {
                    "name": "rmr-route",
                    "container": "id",
                    "port": 4561,
                    "description": "rmr route port for id"
                }
            ]
        },
        "rmr": {
            "protPort": "tcp:4560",
            "maxSize": 2072,
            "numWorkers": 1,
            "rxMessages": ["FBS_ANOMALY_ACK"],
            "txMessages": ["FBS_ANOMALY_UPDATE"],
            "policies": []
        }

}

